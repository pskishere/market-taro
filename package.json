    "upgradePeerdeps": "install-peerdeps @tarojs/taro-rn@4.0.7 -o -P && install-peerdeps @tarojs/components-rn@4.0.7 -o -P && install-peerdeps @tarojs/router-rn@4.0.7 -o -P && pod-install"
